{
  "name": "@shopify/plugin-ngrok",
  "version": "0.2.7",
  "description": "Enables the creation of ngrok tunnels from `shopify app dev`, allowing previews from any device",
  "type": "module",
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public",
    "@shopify:registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build": "tsc",
    "prepack": "cross-env NODE_ENV=production yarn run build",
    "test": "vitest run"
  },
  "dependencies": {
    "@oclif/core": "1.3.6",
    "ngrok": "https://github.com/Shopify/ngrok#0d1b1621b998c28ab2bc50f45a897799c6def417"
  },
  "peerDependencies": {
    "@shopify/cli-kit": "^3.0.0"
  },
  "devDependencies": {
    "@shopify/cli-kit": "^3.0.0",
    "vitest": "0.8.1"
  },
  "engine-strict": true,
  "engines": {
    "node": "^14.13.1 || ^16.0.0 || ^17.0.0 || ^18.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "oclif": {
    "commands": "dist/commands",
    "topics": {
      "ngrok": {
        "description": "Manage ngrok tunnels"
      }
    }
  }
}
